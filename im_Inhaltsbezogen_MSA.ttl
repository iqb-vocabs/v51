@prefix dct: <http://purl.org/dc/terms/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>. 
@prefix n: <https://w3id.org/iqb/v51/im/>. 

n:f7t
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.1";
	skos:broader n:g7b;
	skos:prefLabel "nutzen sinntragende Vorstellungen von rationalen Zahlen, insbesondere von natürlichen, ganzen und gebrochenen Zahlen entsprechend der Verwendungsnotwendigkeit,"@de.
n:x2q
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.2";
	skos:broader n:g7b;
	skos:prefLabel "nutzen sinntragende Vorstellungen von reellen Zahlen (z. B. Vollständigkeit der Zahlengerade),"@de.
n:a4x
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.3";
	skos:broader n:g7b;
	skos:prefLabel "nutzen sinntragende Vorstellungen von Operationen rationaler Zahlen (z. B. schrittweiser, halbschriftlicher Verfahren),"@de.
n:d3q
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.4";
	skos:broader n:g7b;
	skos:prefLabel "untersuchen Zahlen nach ihren Faktoren, in einfachen Fällen ohne digitale Mathematikwerkzeuge,"@de.
n:e2b
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.5";
	skos:broader n:g7b;
	skos:prefLabel "stellen Zahlen der Situation angemessen dar, unter anderem in Zehnerpotenzschreibweise,"@de.
n:q7y
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.6";
	skos:broader n:g7b;
	skos:prefLabel "rechnen mit natürlichen, ganzen und rationalen Zahlen, die im täglichen Leben vorkommen, sowohl zur Kontrolle als auch im Kopf und erklären die Bedeutung der Rechenoperationen,"@de.
n:v5r
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.7";
	skos:broader n:g7b;
	skos:prefLabel "begründen die Notwendigkeit von Zahlbereichserweiterungen von N nach Z und Q sowie von Q nach R an Beispielen,"@de.
n:r4d
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.8";
	skos:broader n:g7b;
	skos:prefLabel "erläutern an Beispielen die verschiedenen Vorstellungen zum Bruchbegriff (insbesondere Teile eines oder mehrerer Ganzer, relative Anteile),"@de.
n:f5h
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.9";
	skos:broader n:g7b;
	skos:prefLabel "nutzen Rechengesetze (z. B. Kommutativ-, Assoziativ-, Distributivgesetz), auch zum vorteilhaften Rechnen,"@de.
n:s4s
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.10";
	skos:broader n:g7b;
	skos:prefLabel "nutzen Überschlagsrechnungen zur Orientierung und zur Kontrolle,"@de.
n:b4p
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.11";
	skos:broader n:g7b;
	skos:prefLabel "runden Zahlen dem Sachverhalt entsprechend sinnvoll,"@de.
n:h3q
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.12";
	skos:broader n:g7b;
	skos:prefLabel "prüfen und interpretieren Ergebnisse, auch in Sachsituationen,"@de.
n:d7d
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.13";
	skos:broader n:g7b;
	skos:prefLabel "erläutern an Beispielen den Zusammenhang zwischen Rechenoperationen und deren Umkehrungen und nutzen diese Zusammenhänge,"@de.
n:b3c
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.14";
	skos:broader n:g7b;
	skos:prefLabel "verwenden Prozent- und Zinsrechnung vorstellungsbasiert (z. B. Prozentstreifen) und sachgerecht,"@de.
n:c6y
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.15";
	skos:broader n:g7b;
	skos:prefLabel "erläutern Potenzen und Wurzeln und berechnen einfache Beispiele ohne digitale Medien,"@de.
n:g7x
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.16";
	skos:broader n:g7b;
	skos:prefLabel "beschreiben die Bedeutung des Logarithmierens als eine Umkehrung des Potenzierens und berechnen einfache Beispiele,"@de.
n:a6r
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.17";
	skos:broader n:g7b;
	skos:prefLabel "wählen, beschreiben und bewerten Vorgehensweisen und Verfahren, denen Algorithmen bzw. Kalküle zu Grunde liegen und führen diese aus (z. B. schriftliche Division sowie bei Wurzeln und Potenzen),"@de.
n:f6r
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.18";
	skos:broader n:g7b;
	skos:prefLabel "implementieren ein algorithmisches Verfahren (z. B. Heron-Verfahren zur Bestimmung von Quadratwurzeln, Intervallschachtelung) mit digitalen Mathematikwerkzeugen,"@de.
n:d7z
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.19";
	skos:broader n:g7b;
	skos:prefLabel "führen in konkreten Situationen systematische Zählprinzipien aus (z. B. Anzahl Händeschütteln, wenn man jeder Person die Hand gibt),"@de.
n:v3q
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1.20";
	skos:broader n:g7b;
	skos:prefLabel "führen Zahlenfolgen fort und verwenden Variablen als allgemeine Zahl."@de.
n:g7b
	 a skos:Concept;
	skos:inScheme n:;
	skos:notation "1";
	skos:topConceptOf n:;
	skos:prefLabel "Leitidee Zahl und Operation"@de;
